  "scripts": {
    "dev": "NODE_ENV=development npx quartz build --concurrency 6 --serve --verbose",
    "prod": "NODE_ENV=production npx quartz build --concurrency 6 --verbose && npm run cf",
    "docs": "npx quartz build --serve -d docs",
    "check": "tsc --noEmit && npx prettier . --check",
    "format": "npx prettier . --write",
    "test": "tsx --test",
    "profile": "0x -D prof ./quartz/bootstrap-cli.mjs build --concurrency=1",
    "cf": "npx wrangler pages dev",
    "vercel": "npx vercel dev",
    "vendorred": "go run quartz/scripts/format.go -dir=sites",
    "convert": "tsx quartz/scripts/convert.ts"
  },
