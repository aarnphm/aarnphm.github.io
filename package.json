  "scripts": {
    "dev": "NODE_ENV=development pnpm exec quartz/bootstrap-cli.mjs build --concurrency 8 --serve --verbose",
    "bundle": "pnpm exec quartz/bootstrap-cli.mjs build --concurrency 8 --bundleInfo --verbose",
    "bundle:dev": "CF_PAGES=1 NODE_ENV=development pnpm bundle",
    "prod": "NODE_ENV=production pnpm bundle",
    "test": "tsx --test",
    "swarm": "pnpx tsx quartz/scripts/dev.ts",
    "convert": "tsx quartz/scripts/convert.ts",
    "cf:deploy": "pnpm check && pnpm dlx wrangler deploy",
    "check": "pnpm format && pnpm convert && pnpm dlx wrangler types && pnpm prettier . --check && tsc --noEmit && tsx --test",
    "format": "pnpx bibtex-tidy content/References.bib --months --merge=combine --no-align --sort=type,year,eprint --duplicates=key,doi,citation --no-escape --sort-fields=title,shorttitle,author,year,month,day,journal,booktitle,eprint,archivePrefix,primaryClass,location,on,publisher,address,series,volume,number,pages,doi,isbn,issn,url,urldate,copyright,category,note,metadata --strip-comments --no-remove-dupe-fields --blank-lines --modify && pnpm prettier . --write",
    "profile": "0x -D prof quartz/bootstrap-cli.mjs build --concurrency=1"
  },
  "engines": {
    "npm": ">=9.3.1",
    "node": ">=22"
  },
  "keywords": [
    "site generator",
    "ssg",
    "digital-garden",
    "markdown",
    "blog",
    "quartz"
  ],
  "bin": {
    "quartz": "./quartz/bootstrap-cli.mjs"
  },
