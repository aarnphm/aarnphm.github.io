{
  "extends": "../tsconfig.json",
  "compilerOptions": {
    "types": ["../worker-configuration.d.ts"],
    "lib": ["esnext", "webworker"],
    "module": "esnext",
    "target": "esnext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "allowJs": true,
    "checkJs": false,
    "noEmit": true
  },
  "include": ["**/*.ts", "**/*.tsx"],
  "exclude": ["../build/**/*.d.ts"]
}
