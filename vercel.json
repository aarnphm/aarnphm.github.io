{
  "cleanUrls": true,
  "framework": null,
  "installCommand": "npm install",
  "buildCommand": "",
  "outputDirectory": "content",
  "redirects": [
    {
      "source": "/",
      "destination": "https://aarnphm.xyz/",
      "permanent": false
    },
    {
      "source": "/dump",
      "destination": "https://aarnphm.xyz/thoughts",
      "permanent": false
    },
    {
      "source": "/dump/(.*)",
      "destination": "https://aarnphm.xyz/thoughts/$1",
      "permanent": true
    }
  ]
}
